<?xml version="1.0" encoding="UTF-8"?>
<Patch>
    <!-- Patch for Vanilla Psycasts Expanded -->
     
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>[LFS] Genes Expanded - Eyes</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/XenotypeDef[defName="ToR_Khun"]/genes</xpath>
                    <value>
                        <li MayRequire="LazyFridayStudio.GenesExpandedEyes">Eyes_Teal</li>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/XenotypeDef[defName="ToR_Khun"]/genes</xpath>
                    <value>
                        <li MayRequire="LazyFridayStudio.GenesExpandedEyes">Eyes_Denim</li>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/XenotypeDef[defName="ToR_Khun"]/genes</xpath>
                    <value>
                        <li MayRequire="LazyFridayStudio.GenesExpandedEyes">Eyes_Bluegill</li>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/XenotypeDef[defName="ToR_Khun"]/genes</xpath>
                    <value>
                        <li MayRequire="LazyFridayStudio.GenesExpandedEyes">Eyes_BlueSuede</li>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/XenotypeDef[defName="ToR_Khun"]/genes</xpath>
                    <value>
                        <li MayRequire="LazyFridayStudio.GenesExpandedEyes">Eyes_SummerSky</li>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>